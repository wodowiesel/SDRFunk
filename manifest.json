    "docs/img-src/stereo.svg",
    "docs/favicon.ico",
    "docs/favicon.png",
    "docs/favicon.svg"
   ],

   "options_ui":
   {
     "browser_style": true,
     "page": "html/options.html",
     "open_in_tab": true
   },

   "file_system_provider_capabilities":
   {
   "configurable": true,
   "multiple_mounts": true,
   "source": "network"
   },

   "content_scripts":
   [
    {
    "all_frames": true,
    "matches": ["<all_urls>"],
    "js": ["js/interface.js", "js/main.js"]
    }
   ],

   "page_action":
   {
     "default_icon":
     {
       "32": "assets/icons/icon32.png"
     },
     "default_title": "SDRFunk",
     "default_popup": "html/popup.html"
   },

   "browser_action":
   {
   "default_icon": "assets/icons/icon32.png",
   "default_title": "SDRFunk",
   "default_popup": "html/popup.html"
   },

   "commands":
   {
     "toggle-feature":
     {
       "description": "Toggle Radio on/off",
       "suggested_key":
       {
         "default": "MediaPlayPause"
       }
     }
   },

   "omnibox":
   {
     "keyword": "sdr"
   },

   "bluetooth": {"uuids": ["1105", "1006"]},
   "sockets":
   {
    "tcp": {"connect": "*"},
    "udp": {"send": "*"}
   },

   "optional_permissions":
   [
     "cookies", "downloads", "sessions", "topSites", "sessions", "history",
     "management", "pageCapture", "webNavigation", "webRequest",
